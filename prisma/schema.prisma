generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
// ColorDesc     String
// ReverseCode   String?

model BlindFormula {
  Type          String
  Code          String
  ThinkCode     String
  Formula       String
  FormulaKey    String  @id @default(uuid())
  MirrorFormula String?
  MirrorCode    String?

  @@map("BlindFormula")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model BlindFormulaCode {
  Code      String @id
  Color     String
  Type      String
  LayerCode String

  @@map("BlindFormulaCode")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model BlindFormulaGroup {
  GroupName   String
  FormulaCode String
  FormulaKey  String
  GroupKey    String @id @default(uuid())

  @@map("BlindFormulaGroup")
}

model CFOP {
  Type         String?
  Formula      String?
  OtherFormula String?
  ID           String  @id @default(uuid())
  Name         String?
  Code         String?

  @@map("CFOP")
}
